{
  "name": "Omnipreneur Protocol - Complete Business Launch Workflow",
  "nodes": [
    {
      "id": "1",
      "name": "Business Goal Input",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [240, 300],
      "parameters": {
        "httpMethod": "POST",
        "path": "omnipreneur-input",
        "responseMode": "responseNode",
        "options": {}
